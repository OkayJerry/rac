{
  "name": "@rac/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "tree": "tree -a -I \"$(grep -vE '^#|^$' .gitignore | tr '\\n' '|' | sed 's/|$//')|.git\" -o tree.txt",
    "dev": "concurrently \"npx nx serve extension\" \"wait-on http://localhost:4200 && open -n -a 'Google Chrome' --args --load-extension=$(pwd)/extension/dist --disable-extensions-except=$(pwd)/extension/dist\"",
    "clean": "npx nx reset",
    "clean:dist": "rimraf dist && rimraf test-output",
    "format": "npx nx format:write --all",
    "lint": "npx nx run-many --target=lint --all --fix",
    "affected:lint": "npx nx affected --target=lint --base=origin/main --parallel",
    "build": "npx nx run-many --target=build --all",
    "affected:build": "npx nx affected --target=build --base=origin/main --parallel",
    "test:unit": "npx nx run-many --target=test --all",
    "test:e2e": "firebase emulators:exec \"npx nx e2e extension-e2e\"",
    "test": "npm run test:unit && npm run test:e2e",
    "affected:test": "npx nx affected --target=test --base=origin/main --parallel",
    "emulators": "firebase emulators:start --only auth,firestore,functions",
    "deploy:functions": "npx nx run functions:deploy",
    "dep-graph": "npx nx dep-graph --file=graph.html",
    "ci:affected": "npm run clean && npm run affected:lint && npm run affected:build && npm run affected:test",
    "ci": "npm run clean && npm run lint && npm run build && npm run test"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@crxjs/vite-plugin": "^2.0.2",
    "@eslint/js": "^9.8.0",
    "@nx-toolkits/firebase": "^0.9.2",
    "@nx/devkit": "21.2.2",
    "@nx/esbuild": "latest",
    "@nx/eslint": "21.2.2",
    "@nx/eslint-plugin": "21.2.2",
    "@nx/jest": "latest",
    "@nx/js": "21.2.2",
    "@nx/playwright": "^21.2.2",
    "@nx/react": "^21.2.2",
    "@nx/vite": "21.2.2",
    "@nx/web": "21.2.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@testing-library/dom": "10.4.0",
